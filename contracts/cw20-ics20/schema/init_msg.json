{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "default_timeout",
    "balances"
  ],
  "properties": {
    "default_timeout": {
      "description": "Default timeout for ics20 packets, specified in seconds",
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "balances": {
      "description": "balances",
      "type": "array",
      "items": {
        "$ref": "#/definitions/balance"
      }
    }
  },
  "definitions": {
    "balance": {
      "description": "balance",
      "properties": {
        "address": {
          "type": "string"
        },
        "amount": {
          "type": "integer",
          "format": "uint128"
        }
      }
    }
  }
}
